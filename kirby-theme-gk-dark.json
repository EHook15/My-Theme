{
    "name": "Galacta Knight Dark",
    "type": "dark",
    "colors": {
      "editor.background": "#12101A",
      "editor.foreground": "#E0E0E0",
      "editorLineNumber.foreground": "#5A4B72",
      "editorCursor.foreground": "#FF5FD7",
      "editor.selectionBackground": "#FF5FD733",
      "editor.inactiveSelectionBackground": "#FF5FD71A",
      "editorIndentGuide.background": "#2D2A40",
      "editorIndentGuide.activeBackground": "#B388FF",
      
      "terminal.background": "#181520",
      "terminal.foreground": "#E0CCE0",
      "terminalCursor.foreground": "#FF5FD7",
  
      "activityBar.background": "#1B1828",
      "sideBar.background": "#16141F",
      "statusBar.background": "#1B1828",
  
      "tab.activeBackground": "#1E1B2A",
      "tab.inactiveBackground": "#181520",
      "tab.activeForeground": "#FF5FD7",
      "tab.border": "#FF5FD7",
      "tab.activeBorderTop": "#FF5FD7",
      "tab.unfocusedActiveBorderTop": "#B388FF",
      "editorGroupHeader.tabsBackground": "#181520",
      "editorGroupHeader.noTabsBackground": "#181520",
  
      "titleBar.activeBackground": "#1B1828",
      "titleBar.inactiveBackground": "#1B1828",
  
      "breadcrumb.foreground": "#A08CB0",
      "breadcrumb.background": "#181520"
    },
    "tokenColors": [
      {
        "scope": ["comment", "punctuation.definition.comment"],
        "settings": {
          "foreground": "#9D6F9D",
          "fontStyle": "italic"
        }
      },
      {
        "scope": ["keyword", "storage.type"],
        "settings": {
          "foreground": "#FF5FD7",
          "fontStyle": "bold"
        }
      },
      {
        "scope": ["constant", "variable.language"],
        "settings": {
          "foreground": "#FFB3FD",
          "fontStyle": "bold"
        }
      },
      {
        "scope": ["string", "constant.other.symbol"],
        "settings": {
          "foreground": "#C792EA"
        }
      },
      {
        "scope": ["entity.name.function", "support.function"],
        "settings": {
          "foreground": "#B388FF"
        }
      },
      {
        "scope": ["variable"],
        "settings": {
          "foreground": "#E0E0E0"
        }
      },
      {
        "scope": ["entity.name.type", "support.class"],
        "settings": {
          "foreground": "#FF9BE6"
        }
      },
      {
        "scope": ["invalid", "invalid.deprecated"],
        "settings": {
          "foreground": "#FFFFFF",
          "background": "#FF6E96"
        }
      }
    ]
  }